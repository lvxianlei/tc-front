    "contractBaseInfo": [
        {
            "title": "合同编号",
            "dataIndex": "contractNumber",
            "disabled": true
        },
        {
            "title": "关联采购计划",
            "dataIndex": "purchasePlan",
            "type": "popTable",
            "path": "/tower-supply/materialPurchasePlan",
            "width": 1011,
            "value": "purchasePlanCode",
            "dependencies": true,
            "readOnly": true,
            "columns": [
                {
                    "title": "采购计划编号",
                    "dataIndex": "purchasePlanCode",
                    "width": 200,
                    "type": "string",
                    "search": true
                },
                {
                    "title": "采购类型",
                    "dataIndex": "purchaseType",
                    "type": "select",
                    "width": 100,
                    "search": true,
                    "enum": [
                        {
                            "label": "外部",
                            "value": 1
                        },
                        {
                            "label": "内部",
                            "value": 2
                        },
                        {
                            "label": "缺料",
                            "value": 3
                        }
                    ]
                },
                {
                    "title": "塔型（个）",
                    "dataIndex": "productCategoryNum",
                    "width": 100,
                    "type": "integer"
                },
                {
                    "title": "杆塔（基）",
                    "dataIndex": "productNum",
                    "width": 100,
                    "type": "integer"
                }
            ],
            "rules": [
                {
                    "required": true,
                    "message": "请关联采购计划..."
                }
            ]
        },
        {
            "title": "供应商",
            "dataIndex": "supplier",
            "type": "popTable",
            "path": "/tower-supply/supplier",
            "width": 1011,
            "value": "supplierName",
            "dependencies": true,
            "readOnly": true,
            "columns": [
                {
                    "title": "供应商编号",
                    "dataIndex": "supplierCode"
                },
                {
                    "title": "供应商名称",
                    "dataIndex": "supplierName"
                },
                {
                    "title": "联系人",
                    "dataIndex": "contactMan"
                },
                {
                    "title": "联系电话",
                    "dataIndex": "contactNumber"
                },
                {
                    "title": "供货产品",
                    "dataIndex": "supplyProducts"
                }
            ],
            "rules": [
                {
                    "required": true,
                    "message": "请选择供应商..."
                }
            ]
        },
        {
            "title": "签订时间",
            "dataIndex": "signingTime",
            "type": "date"
        },
        {
            "title": "经办人",
            "dataIndex": "operator"
        },
        {
            "title": "交货方式",
            "dataIndex": "deliveryMethod",
            "type": "select"
        },
        {
            "title": "材料标准",
            "dataIndex": "materialStandard",
            "type": "select"
        },
        {
            "title": "运输承担",
            "dataIndex": "transportBear",
            "type": "select",
            "enum": [
                {
                    "value": 1,
                    "label": "供方"
                },
                {
                    "value": 2,
                    "label": "需方"
                }
            ]
        },
        {
            "title": "运输方式",
            "dataIndex": "transportMethod",
            "type": "select"
        },
        {
            "title": "卸车承担",
            "dataIndex": "unloadBear",
            "type": "select",
            "enum": [
                {
                    "value": 1,
                    "label": "供方"
                },
                {
                    "value": 2,
                    "label": "需方"
                }
            ]
        },
        {
            "title": "备注",
            "dataIndex": "description",
            "type": "textarea"
        }
    ],
    "comparison": [
        {
            "title": "询比价信息",
            "dataIndex": "comparisonPrice",
            "type": "popTable",
            "path": "/tower-supply/comparisonPrice",
            "width": 1011,
            "value": "comparisonNumber",
            "dependencies": true,
            "readOnly": true,
            "columns": [
                {
                    "title": "询比价编号",
                    "dataIndex": "comparisonNumber"
                },
                {
                    "title": "用途",
                    "dataIndex": "purpose"
                },
                {
                    "title": "询比价状态",
                    "dataIndex": "comparisonStatus",
                    "type": "select",
                    "enum": [
                        {
                            "label": "询价中",
                            "value": 1
                        },
                        {
                            "label": "已询价",
                            "value": 2
                        },
                        {
                            "label": "取消",
                            "value": 3
                        }
                    ]
                },
                {
                    "title": "最新状态变更时间",
                    "dataIndex": "updateTime"
                },
                {
                    "title": "询价人",
                    "dataIndex": "comparisonPersonId"
                }
            ],
            "rules": [
                {
                    "required": true,
                    "message": "请选择供应商..."
                }
            ]
        }
    ],
    "addMaterial": {
        "title": "原材料",
