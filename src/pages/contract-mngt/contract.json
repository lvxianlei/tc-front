                "dataIndex": "standard"
            },
            {
                "title": "材料",
                "dataIndex": "rowMaterial"
            },
            {
                "title": "材质",
                "dataIndex": "materialTexture"
            },
            {
                "title": "规格",
                "dataIndex": "spec"
            }
        ]
    }
